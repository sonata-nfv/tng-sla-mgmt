    "paths": {
	  "/api/slas/v1/templates": {
      "post": {
        "responses": {
          "404": {
            "description": "Not Found: NS not found."
          },
          "201": {
            "description": "Created"       
          }
        },
        "operationId": "create_template",
        "parameters": [
          {
            "name": "nsd_uuid",
            "in": "body",
            "type": "String",
            "required": true,
			 "schema": {
              "description": "Network Service Descriptor UUID",
              "type": "object",
              "properties": {
                "timeStamp": {
                  "description": "Date-time stamp. Representation: String formatted according to IETF RFC 3339.\n",
                  "type": "string",
                  "format": "date-time"
                },
                "id": {
                  "description": "NSD Identifier.\n",
                  "type": "string"
                }
              }
            },
            "description": "Network Service UUID"
          },
          {
            "name": "templateName",
            "in": "body",
            "type": "string",
			"required": true,			
            "description": "Name of the SLA template"
          },
          {
            "name": "expireDate",
            "in": "body",
            "type": "string",
			"required": true,
            "description": "Expire date of the SLA"
          },
          {
            "name": "guaranteeID",
            "in": "body",
            "type": "string",
			"required": true,
            "description": "The ID of the SLO",
            "default": "g1"
          }
        ],
        "consumes": [
          "multipart/form-data"
        ],
        "tags": [
          "v1"
        ]
      }
    },
