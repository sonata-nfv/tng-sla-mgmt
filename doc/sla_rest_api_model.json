    "paths": {
	  "/api/slas/v1/templates/{nsd_uuid}": {
      "post": {
        "responses": {
          "404": {
            "description": "Not Found: NS uuid not found."
          },
          "200": {
            "description": "Success"       
          }
        },
        "operationId": "create_template",
        "parameters": [
          {
            "name": "nsd_uuid",
            "in": "path",
            "type": "String",
            "required": true,
            "description": "Network Service UUID"
          },
          {
            "name": "templateName",
            "in": "path",
            "type": "string",
			"required": true,
            "description": "Name of the SLA template"
          },
          {
            "name": "expireDate",
            "in": "path",
            "type": "string",
			"required": true,
            "description": "Expire date of the SLA"
          },
          {
            "name": "guaranteeID",
            "in": "formData",
            "type": "string",
			"required": true,
            "description": "The ID of the SLO",
            "default": "g1"
          }
        ],
        "consumes": [
          "multipart/form-data"
        ],
        "tags": [
          "v1"
        ]
      }
    },
